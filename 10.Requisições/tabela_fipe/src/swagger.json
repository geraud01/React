  "components": {
    "schemas": {
      "VehiclesType": {
        "type": "string",
        "enum": [
          "cars",
          "motorcycles",
          "trucks"
        ]
      },
      "FipeResult": {
        "type": "object",
        "properties": {
          "price": {
            "type": "string",
            "description": "Price in Brazilian Real"
          },
          "brand": {
            "type": "string",
            "description": "Brand of the vehicle"
          },
          "model": {
            "type": "string",
            "description": "Model of the vehicle"
          },
          "modelYear": {
            "type": "integer",
            "format": "int32",
            "description": "Year of vehicle production"
          },
          "fuel": {
            "type": "string",
            "description": "Fuel used by the vehicle"
          },
          "codeFipe": {
            "type": "string",
            "description": "Unique Fipe identifier"
          },
          "referenceMonth": {
            "type": "string",
            "description": "Month of the price"
          },
          "vehicleType": {
            "type": "integer",
            "format": "int32",
            "description": "Type of the vehicle"
          },
          "fuelAcronym": {
            "type": "string",
            "description": "Fuel acronym (eg. G)"
          }
        }
      },
      "NamedCode": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string"
          },
          "code": {
            "type": "string"
          }
        }
      },
      "Reference": {
        "type": "object",
        "properties": {
          "code": {
            "type": "string"
          },
          "month": {
            "type": "string"
          }
        }
      },
      "FipeHistoryResult": {
        "type": "object",
        "properties": {
          "brand": {
            "type": "string",
            "description": "Brand of the vehicle"
          },
          "model": {
            "type": "string",
            "description": "Model of the vehicle"
          },
          "modelYear": {
            "type": "integer",
            "format": "int32",
            "description": "Year of vehicle production"
          },
          "fuel": {
            "type": "string",
            "description": "Fuel used by the vehicle"
          },
          "codeFipe": {
            "type": "string",
            "description": "Unique Fipe identifier"
          },
          "vehicleType": {
            "type": "integer",
            "format": "int32",
            "description": "Type of the vehicle"
          },
          "fuelAcronym": {
            "type": "string",
            "description": "Fuel acronym (eg. G)"
          },
          "priceHistory": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/HistoryEntry"
            }
          }
        }
      },
      "HistoryEntry": {
        "type": "object",
        "properties": {
          "price": {
            "type": "string"
          },
          "month": {
            "type": "string"
          },
          "reference": {
            "type": "string"
          }
        }
      }
    }
  }
}