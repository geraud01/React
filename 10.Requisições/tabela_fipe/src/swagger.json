    "/{vehicleType}/brands/{brandId}/models/{modelId}/years/{yearId}": {
      "get": {
        "tags": [
          "Fipe"
        ],
        "summary": "Fipe info",
        "description": "Returns the Fipe information for the vehicle (price estimation)",
        "operationId": "GetFipeInfo",
        "parameters": [
          {
            "name": "vehicleType",
            "in": "path",
            "description": "Type of vehicle",
            "required": true,
            "example": "cars",
            "schema": {
              "$ref": "#/components/schemas/VehiclesType"
            }
          },
          {
            "name": "brandId",
            "in": "path",
            "description": "Brand of the vehicle",
            "required": true,
            "example": 59,
            "schema": {
              "type": "integer",
              "format": "int32"
            }
          },
          {
            "name": "modelId",
            "in": "path",
            "description": "Model of the vehicle",
            "required": true,
            "example": 5940,
            "schema": {
              "type": "integer",
              "format": "int32"
            }
          },
          {
            "name": "yearId",
            "in": "path",
            "description": "Year for the vehicle",
            "required": true,
            "example": "2014-3",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "reference",
            "in": "query",
            "description": "Month reference code",
            "example": "278",
            "schema": {
              "type": "integer",
              "format": "int32"
            }
          }
        ],
        "responses": {
