  "paths": {
    "/references": {
      "get": {
        "tags": [
          "Fipe"
        ],
        "summary": "Fipe month references",
        "description": "Returns months and codes reference from Fipe",
        "operationId": "GetReferences",
        "responses": {
          "200": {
            "description": "Ok",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/Reference"
                  }
                }
              }
            }
          },
          "500": {
            "description": "Server Error",
            "content": {}
          }
        }
      }
    },
    "/{vehicleType}/brands": {
      "get": {
        "tags": [
          "Fipe"
        ],
        "summary": "Brands by type",
        "description": "Returns brands for the type of vehicle",
        "operationId": "GetBrandsByType",
        "parameters": [
          {
            "name": "vehicleType",
            "in": "path",
            "description": "Type of vehicle",
            "required": true,
            "schema": {
              "$ref": "#/components/schemas/VehiclesType"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Ok",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/NamedCode"
                  }
                }
              }
            }
          },
          "404": {
            "description": "Not found",
            "content": {}
          }
        }
      }
    },
